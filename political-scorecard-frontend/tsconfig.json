{
  "compilerOptions":{
    "lib": [
      "dom",
      "dom.iterable"
    ],
    "types" : ["cypress"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict":false,
    "noEmit":true,
    "incremental": true,
    "esModuleInterop":true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules":true,
    "jsx":"preserve",
    "allowImportingTsExtensions":true
  },
  "include":[
    "**/*.ts",
    "**/*.tsx",
  ],
  "exclude":[
    "node_modules"
  ]
}
